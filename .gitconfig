[user]
    name = taqun
    email = envoixnet@gmail.com
[credential]
    helper = osxkeychain
[color]
    ui = auto
[core]
    excludesfile = ~/.gitignore
	editor = /usr/local/bin/emacsclient --alternate-editor /usr/local/bin/emacs
[alias]
    st = status
    co = checkout
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[push]
	default = simple
